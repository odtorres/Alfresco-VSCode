	"constraint REGEX": {
		"prefix": "constraint REGEX",
		"body": "\r\n<constraint name=\"$1:$2\" type=\"REGEX\">\r\n   <parameter name=\"expression\">\r\n      <value><![CDATA[(.*[\\\"\\*\\\\\\>\\<\\?\\/\\:\\|]+.*)|(.*[\\.]?.*[\\.]+$)|(.*[ ]+$)] ]></value>\r\n   </parameter>\r\n   <parameter name=\"requiresMatch\"><value>false</value></parameter>\r\n</constraint>\r\n",
		"description": "ContenModel.xml",
		"scope": "text.xml"
	},
