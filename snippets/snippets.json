	"config actions": {
		"prefix": "config actions",
		"body": "\r\n<config evaluator=\"string-compare\" condition=\"DocLibActions\">\r\n\t<actions>\r\n\t\t<!-- type=\"javascript\" -->\r\n\t\t<action id=\"add-element\" icon=\"add-element\" type=\"javascript\" label=\"label.action.add-element\">\r\n\t\t\t<param name=\"function\">onActionFormDialog</param>\r\n\t\t\t<param name=\"itemKind\">action</param>\r\n\t\t\t<param name=\"itemId\">add-element</param>\r\n\t\t\t<param name=\"mode\">create</param>\r\n\t\t\t<param name=\"destination\">{node.nodeRef}</param>\r\n\t\t\t<param name=\"successMessage\">message.add-element.success</param>\r\n\t\t\t<param name=\"failureMessage\">message.add-element.failure</param>\r\n\t\t</action>\r\n\t\t<!--  type=\"pagelink\"  -->\r\n\t\t<action id=\"manage-element\" type=\"pagelink\" icon=\"manage-element\" label=\"label.actions.manage-element\">\r\n            <param name=\"page\">manage-element?nodeRef={node.nodeRef}</param>\r\n         </action>\r\n         <!--  type=\"link\"  -->\r\n\t\t<action id=\"manage-element\" type=\"link\" icon=\"manage-element\" label=\"label.actions.manage-element\">\r\n            <param name=\"page\">manage-element?nodeRef={node.nodeRef}</param>\r\n         </action>\r\n\t</actions>\r\n\t<actionGroups>\r\n\t\t<actionGroup id=\"document-browse\">\r\n\t\t\t<action index=\"110\" id=\"add-element\"/>\r\n\t\t\t<action index=\"110\" id=\"manage-element\"/>\r\n\t\t\t</actionGroup>\r\n\t\t<actionGroup id=\"document-details\">\r\n\t\t\t<action index=\"110\" id=\"add-element\"/>\r\n\t\t\t<action index=\"110\" id=\"manage-element\"/>\r\n\t\t</actionGroup>\r\n\t\t<actionGroup id=\"folder-browse\">\r\n\t\t\t<action index=\"102\" id=\"add-element\"/>\r\n\t\t\t<action index=\"125\" id=\"manage-element\"/>\r\n\t\t</actionGroup>\r\n\t\t<actionGroup id=\"folder-details\">\r\n\t\t\t<action index=\"102\" id=\"add-element\"/>\r\n\t\t\t<action index=\"125\" id=\"manage-element\"/>\r\n\t\t</actionGroup>\r\n\t</actionGroups>\r\n</config>",
		"description": "Form.xml",
		"scope": "text.xml"
	},
	"config aspects": {
		"prefix": "config aspects",
		"body": "\r\n<config evaluator=\"string-compare\" condition=\"DocumentLibrary\">\r\n  <aspects>   \r\n   <visible>\r\n       <aspect name=\"$1:$2\" />       \r\n   </visible>\r\n\r\n   <addable>\r\n   </addable>\r\n   \r\n   <removeable>\r\n   </removeable>\r\n  </aspects>\r\n</config>",
		"description": "Form.xml",
		"scope": "text.xml"
	},
	"config create-content": {
		"prefix": "config create-content",
		"body": "\r\n<config evaluator=\"string-compare\" condition=\"DocumentLibrary\">\r\n  <create-content>         \r\n    <content id=\"$1\" label=\"$2\" type=\"pagelink\" index=\"10\" icon=\"text\">\r\n      <param name=\"page\">create-content?destination={nodeRef}&amp;itemId=prefix:model&amp;mimeType=text/plain</param>\r\n    </content>\r\n  </create-content>\r\n</config>",
		"description": "Form.xml",
		"scope": "text.xml"
	},
	"config forms": {
		"prefix": "config forms",
		"body": "\r\n<config>\r\n   <forms>\r\n      <default-controls>\r\n         <type name=\"type\" template=\"path\">\r\n            <control-param name=\"name\">value</control-param>\r\n         </type>\r\n      </default-controls>\r\n\r\n      <constraint-handlers>\r\n         <constraint type=\"id\" validation-handler=\"function\" [message-id=\"key\"] [message=\"string\"] [event=\"string\"] />\r\n      </constraint-handlers>\r\n\r\n      <dependencies>\r\n         <js src=\"path\" />\r\n         <css src=\"path\" />\r\n      </dependencies>\r\n   </forms>\r\n</config>\r\n\r\n<config evaluator=\"node-type\" condition=\"type\">\r\n   <forms>\r\n      <form [id=\"string\"] [submission-url=\"url\"]>\r\n         <view-form template=\"path\" />\r\n         <edit-form template=\"path\" />\r\n         <create-form template=\"path\" />\r\n         <field-visibility>\r\n            <show id=\"string\" [for-mode=\"view|edit|create\"] [force=\"boolean\"] />\r\n            <hide id=\"string\" [for-mode=\"view|edit|create\"] />\r\n         </field-visibility>\r\n         <appearance>\r\n            <set id=\"string\" appearance=\"fieldset|panel|bordered-panel|title|whitespace|\" [parent=\"string\"] [label=\"string\"] [label-id=\"key\"] [template=\"path\"] />\r\n\r\n            <field id=\"string\" [label-id=\"key\"] [label=\"string\"] [description-id=\"key\"] [description=\"string\"]\r\n                   [help=\"string\"] [help-id=\"key\"] [read-only=\"boolean\"] [mandatory=\"boolean\"] [set=\"string\"]>\r\n               <control [template=\"path\"]>\r\n                  <control-param name=\"name\">value</control-param>\r\n               </control>\r\n               <constraint-handlers>\r\n                  <constraint type=\"id\" validation-handler=\"function\" [message-id=\"string\"] [message=\"string\"] [event=\"string\"] />\r\n               </constraint-handlers>\r\n            </field>\r\n         </appearance>\r\n      </form>\r\n   </forms>\r\n</config>",
		"description": "Form.xml",
		"scope": "text.xml"
	},
	"config types": {
		"prefix": "config types",
		"body": "\r\n<config evaluator=\"string-compare\" condition=\"DocumentLibrary\">\r\n  <types>\r\n     <type name=\"cm:content\">\r\n         <subtype name=\"$1:$2\" />         \r\n     </type>     \r\n </types>\r\n</config>",
		"description": "Form.xml",
		"scope": "text.xml"
	},
	"constraint handlers": {
		"prefix": "constraint handlers",
