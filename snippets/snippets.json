	"constraint MINMAX": {
		"prefix": "constraint MINMAX",
		"body": "\r\n<constraint name=\"$1:$2\" type=\"MINMAX\">\r\n <parameter name=\"minValue\">\r\n    <value>0</value>\r\n </parameter>\r\n <parameter name=\"maxValue\">\r\n    <value>100</value>\r\n </parameter>\r\n</constraint>",
