	"model": {
		"prefix": "model",
		"body": "<?xml version=\"1.0\" encoding=\"UTF-8\" standalone=\"no\"?>\r\n<model xmlns=\"http://www.alfresco.org/model/dictionary/1.0\" name=\"$1:model\">\r\n\t<description>$TM_FILENAME</description>\r\n\t<author>Oscar Daniel Torres Hdez : odtorres@uci.cu</author>\r\n\t<published>2016-03-18</published>\r\n\t<version>1.0</version>\r\n\t\r\n\t<imports>\r\n\t\t<import prefix=\"d\" uri=\"http://www.alfresco.org/model/dictionary/1.0\" />\r\n\t\t<import prefix=\"cm\" uri=\"http://www.alfresco.org/model/content/1.0\" />\r\n\t\t<import prefix=\"sys\" uri=\"http://www.alfresco.org/model/system/1.0\" />\r\n\t</imports>\r\n\r\n\t<namespaces>\r\n\t\t<namespace prefix=\"$1\" uri=\"http://www.org/v3.1/module/$2/model/1.0\" />\r\n\t</namespaces>\r\n\t\r\n\t<constraints>\r\n\t\t<constraint name=\"$1:x-list\" type=\"LIST\">\r\n\t\t\t<parameter name=\"allowedValues\">\r\n\t\t\t\t<list>\r\n\t\t\t\t\t<value>1</value>\r\n\t\t\t\t\t<value>2</value>\r\n\t\t\t\t\t<value>3</value>\r\n\t\t\t\t</list>\r\n\t\t\t</parameter>\r\n\t\t</constraint>\r\n\t</constraints>\r\n\r\n\t<types>\r\n\t\t<type name=\"$1:name\">\r\n\t\t\t<title>Title</title>\r\n\t\t\t<parent>cm:content</parent>\r\n\t\t\t<properties>\r\n\t\t\t\t<property name=\"$1:date\">\r\n\t\t\t\t\t<title>Date</title>\r\n\t\t\t\t\t<type>d:date</type>\r\n\t\t\t\t</property>\r\n\t\t\t\t<property name=\"$1:list\">\r\n\t\t\t\t\t<title>List</title>\r\n\t\t\t\t\t<type>d:text</type>\r\n\t\t\t\t\t<constraints>\r\n\t\t\t\t\t\t<constraint ref=\"$1:x-list\" />\r\n\t\t\t\t\t</constraints>\r\n\t\t\t\t</property>\r\n\t\t\t\t<property name=\"$1:text\">\r\n\t\t\t\t\t<title>Text</title>\r\n\t\t\t\t\t<type>d:text</type>\r\n\t\t\t\t</property>\t\t\t\t\r\n\t\t\t</properties>\r\n\t\t\t<mandatory-aspects>\r\n\t\t\t\t<aspect>cm:auditable</aspect>\r\n\t\t\t\t<aspect>cm:author</aspect>\r\n\t\t\t</mandatory-aspects>\r\n\t\t</type>\r\n\t</types>\r\n\t\r\n\t<aspects>\r\n\t\t<aspect name=\"$1:x-aspect\">\r\n\t\t\t\r\n\t\t</aspect>\r\n\t</aspects>\r\n</model>",
		"description": "ContenModel.xml",
		"scope": "text.xml"
	},
	"bean cron js": {
		"prefix": "bean cron js",
		"body": "\r\n<!-- Scheduled scripts --> \r\n    <bean id=\"$1\" class=\"org.springframework.scheduling.quartz.JobDetailBean\">\r\n        <property name=\"jobClass\">\r\n            <value>org.alfresco.repo.jscript.ExecuteScriptJob</value>\r\n        </property>\r\n        <property name=\"jobDataAsMap\">\r\n            <map>\r\n                <entry key=\"scriptLocation\">\r\n                    <bean class=\"org.alfresco.repo.jscript.ClasspathScriptLocation\">\r\n                     <constructor-arg>\r\n                        <value>alfresco/module/$2/script/scheduled/$3.js</value>\r\n                     </constructor-arg>\r\n                  </bean>\r\n                </entry>\r\n                <entry key=\"scriptService\">\r\n                    <ref bean=\"ScriptService\"/>\r\n                </entry>\r\n                <entry key=\"authenticationComponent\">\r\n                  <ref bean=\"authenticationComponent\"/>\r\n                </entry>\r\n            </map>\r\n        </property>\r\n    </bean>\r\n \r\n    <bean id=\"$4\" class=\"org.alfresco.util.CronTriggerBean\">\r\n        <property name=\"jobDetail\">\r\n            <ref bean=\"$1\" />\r\n        </property>\r\n        <property name=\"scheduler\">\r\n            <ref bean=\"schedulerFactory\" />\r\n        </property>\r\n        <property name=\"cronExpression\">\r\n            <!-- Execute @ 2:30 am every day -->\r\n            <value>0 * * * * ?</value>\r\n        </property>\r\n    </bean>   \r\n",
		"description": "-context.xml",
		"scope": "text.xml"
	},
