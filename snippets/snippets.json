	"constraint List": {
		"prefix": "constraint List",
		"body": "\r\n<constraint name=\"$1:$2\" type=\"LIST\">\r\n\t<parameter name=\"allowedValues\">\r\n\t\t<list>\r\n\t\t\t<value>1</value>\r\n\t\t\t<value>2</value>\r\n\t\t\t<value>3</value>\r\n\t\t</list>\r\n\t</parameter>\r\n</constraint>\r\n",
